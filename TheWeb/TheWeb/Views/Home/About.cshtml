@model TheWeb.Models.AboutNum
 

@{
    ViewBag.Title = "About";
}

<hgroup class="title">
    <h1>@ViewBag.Title.</h1>
    <h2>@ViewBag.Message</h2>
</hgroup>

<article>
    <p>
        <label>Number Of Student that open Project  :</label>@Model.NumberOfStudentProject
    </p>

    <p>
        <label>Student in the same country with project :</label>@Model.Local

    </p>

    <p>
        <label>Num Project in the site right now :</label>@Model.NumProject
    </p>
</article>



@section Scripts{
    <div id="googleMap" style="width:500px;height:380px;"></div>


    <div>
        <header><h1>The Idea</h1></header>
}
<small>Subtext for header</small>
</div>
    <div>
        <header><h1>Vision</h1></header>
        <small>Subtext for header</small>
    </div>
    <div>
        <header><h1>Contact</h1></header>
        <small>Subtext for header</small>
        <div class="bs-example">
            <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModal" data-whatever="mdo">itzik</button>
            <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModal" data-whatever="fat">tamir</button>
            <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModal" data-whatever="getbootstrap">customer service</button>
            <a target="_blank" title="הצטרפו אלינו לפייסבוק" href="https://he-il.facebook.com/college.org.il"><img src="~/Images/facebook.png" alt="חפשו אותנו בפייסבוק"></a>
            <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">

                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                            <h4 class="modal-title" id="exampleModalLabel">New message<a class="anchorjs-link" href="#exampleModalLabel"><span class="anchorjs-icon"></span></a></h4>
                        </div>
                        <div class="modal-body">
                            <form>
                                <div class="form-group">
                                    <label for="recipient-name" class="control-label">Recipient:</label>
                                    <input type="text" class="form-control" id="recipient-name">
                                </div>
                                <div class="form-group">
                                    <label for="message-text" class="control-label">Message:</label>
                                    <textarea class="form-control" id="message-text"></textarea>
                                </div>
                            </form>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                            <button type="button" class="btn btn-primary">Send message</button>
                        </div>
                    </div>
                </div>
            </div><!-- /.modal -->
        </div>
    </div>
}
