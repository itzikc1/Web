@model IEnumerable<TheWeb.Models.project>

@{
    ViewBag.Title = "Index";
}

@Html.ActionLink("Create New", "Create")


<div class="container">
    <div class="row">
        <div class="col-md-2">
            <div class="input-group" id="adv-search">
                <form>
                    <input type="text" class="form-control" placeholder="Search for name" name="bb" />
                </form>
                <div class="input-group-btn">
                    <div class="btn-group" role="group">
                        <div class="dropdown dropdown-lg">
                            <button type="submit" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><span class="caret"></span></button>
                            <div class="dropdown-menu dropdown-menu-right" role="menu">
                                <form class="form-horizontal" role="form">
                                    <div class="col-md-12">
                                        <div class="form-group">

                                            <label for="contain">name</label>
                                            <input class="form-control" type="text" id="name" placeholder="Search for name" name="name" />
                                        </div>
                                        <div class="form-group">
                                            <label for="contain">local</label>
                                            <input class="form-control" type="text" id="local" placeholder="Search for local" name="local" />
                                        </div>
                                        <div class="form-group">
                                            <label for="contain">email Contact</label>
                                            <input class="form-control" type="text" id="emailContact" placeholder="Search for email Contact" name="emailContact" />
                                        </div>
                                        <button type="submit" class="btn btn-primary"><span class="glyphicon glyphicon-search" aria-hidden="true"></span></button>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>



